#include<iostream>
using namespace std;

void pattern(int N)
{
    for(int i=1;i<=N+1;i++)
    {
        for(int j=1;j<i;j++)
        {
            cout<< j;
        }
        cout<<endl;
    }
}
int main()
{
    int N;
    cin>>N;
    pattern(N);
    return 0;
}


output:

    1
    12
    123
    1234
    12345
